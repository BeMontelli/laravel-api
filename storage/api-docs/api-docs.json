    "components": {
        "schemas": {
            "Category": {
                "title": "Category",
                "description": "Category model",
                "required": [
                    "title"
                ],
                "properties": {
                    "title": {
                        "description": "Category title",
                        "type": "string",
                        "example": "Electronics"
                    },
                    "description": {
                        "description": "Category description",
                        "type": "string",
                        "example": "A category for electronic products."
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Category"
                }
            },
            "Product": {
                "title": "Product",
                "description": "Product model",
                "required": [
                    "name",
                    "description",
                    "price",
                    "image",
                    "stock"
                ],
                "properties": {
                    "name": {
                        "description": "Product name",
                        "type": "string",
                        "example": "Laptop"
                    },
                    "description": {
                        "description": "Product description",
                        "type": "string",
                        "example": "A powerful laptop with high-end specifications."
                    },
                    "price": {
                        "description": "Product price",
                        "type": "number",
                        "format": "float",
                        "example": 999.99
                    },
                    "image": {
                        "description": "URL of the product image",
                        "type": "string",
                        "format": "uri",
                        "example": "images/uploads/product.jpg"
                    },
                    "stock": {
                        "description": "Product stock quantity",
                        "type": "integer",
                        "example": 100
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Product"
                }
            },
            "User": {
                "title": "User",
                "description": "User model",
                "properties": {
                    "name": {
                        "description": "User's name",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "email": {
                        "description": "User's email address",
                        "type": "string",
                        "format": "email",
                        "example": "john@example.com"
                    },
                    "password": {
                        "description": "User's password",
                        "type": "string",
                        "format": "password",
                        "example": "password123"
                    },
                    "profile_photo_url": {
                        "description": "URL of the user's profile photo",
                        "type": "string",
                        "format": "uri",
                        "example": "https://example.com/profile.jpg"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "User"
                }
            }
        }
    }
